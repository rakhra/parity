
  <div class="panel panel-default">
     <div class="panel-heading">
          <span class="label label-danger panel-type pull-right">{{type}}</span>
        <h4>{{name}}</h4>
      </div>
      <div class="panel-body">
        <div class="clearfix"></div>
        
          {{{short_description}}}
        
        <hr>
        <ul class="list-unstyled">
          {{#each dates}}
          <li>
            <p>{{dateFormat start "ddd, MMMM Do, h:mma"}} - {{duration start end}}</p>
          </li>
          {{/each}}
        </ul>
        <hr>
        <div class="input-group">
          <div class="input-group-btn">
          {{#if register}}
            <button class="btn btn-alert" onClick="removeImpression('{{register._id}}')">Unregister</button>
          {{else}}
            <button class="btn btn-primary" onClick="impression('{{type}}','{{_id}}', 'register')">Register Me</button>
          {{/if}}
          <a class="btn" href="/event/{{_id}}">Read More</a>
          </div>
        </div>
      </div>
   </div> 
